% a(n)
% k(n,:)
% d(n)

Z = zeros(1000,1000);
for x = 1:1000
    for z = 1:1000
        Z(x,z) = wave(a, k, d, x*0.1, z*0.1);
    end
end

figure(1)
contour(Z)
figu
surf(Z)

clear a;
clear k;
clear d;
clear x;
clear z;
clear Z;

function y = wave(a, k, d, x, z)

    y = sum(a.* cos(k(1,:).* x + k(2,:).* z + d));

end
